    end
    else begin
      //保存先取得
      Db_RisSvr_Irai.wg_DB_Type := Db_RisSvr_Irai.func_GetType_DB(F_sMsg);
      //保存先未設定
      if Db_RisSvr_Irai.wg_DB_Type = '' then
