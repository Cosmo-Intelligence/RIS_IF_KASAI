      //送信領域の初期化
      wg_SendMsgStream.size := 0;
      //受信電文一時退避初期化
      wg_StringStream.size := 0;
      //継続終了
      wg_Continue := False;
      //実電文長（一時退避）初期化
      ServerRecieveBuf_Len_Work := 0;
      //予定電文長（一時退避）初期化
      ServerRecieveBuf_LenPlan_Work := 0;
      //異常終了の場合
      if not w_Res then begin
        //処理異常:FROMHISINFO登録
        wg_RecvMsgStream.Size := 0;
        wg_RecvMsgStream.WriteString('NG');
        Db_RisSvr_Irai.func_SaveMsgNG(Db_RisSvr_Irai.wg_Kind, '', w_RecvMsgTime, wg_RecvMsgStream, F_sMsg);
        //10.2ndログ出力
        proc_LogOut(G_LOG_LINE_HEAD_NG,'',G_LOG_KIND_NG_NUM,F_sMsg);
        //NG電文作成
        Db_RisSvr_Irai.proc_MakeMsg(G_MSG_SYSTEM_A, G_MSGKIND_START,
                                    CST_DENBUNID_NG, ws_Command,
                                    wg_SendMsgStream);
        //11.NG電文送信
        proc_SendAckStream(Sender,Socket);
        //一単位電文クリア但し現電文は残す 強制読込みのため
        //一単位電文の受信終了を意味する
        ServerRecieveBuf_LenPlan := 0;
        ServerRecieveBuf_Len := 0;
        wg_MaxDataSize := 0;
        //受信領域の初期化
        wg_RecvMsgStream.size := 0;
        //送信領域の初期化
        wg_SendMsgStream.size := 0;
        //受信電文一時退避初期化
        wg_StringStream.size := 0;
        //継続終了
        wg_Continue := False;
        //実電文長（一時退避）初期化
        ServerRecieveBuf_Len_Work := 0;
        //予定電文長（一時退避）初期化
        ServerRecieveBuf_LenPlan_Work := 0;
        //エラーログ文字列初期化
        F_sMsg := '';
        //処理終了
        Exit;
      end;
      //10.オーダ登録
      w_Res := Db_RisSvr_Irai.func_UpdateRisDBIrai(F_sMsg);
      //異常終了の場合
      if not w_Res then begin
        //処理異常:FROMHISINFO登録
        wg_RecvMsgStream.Size := 0;
        wg_RecvMsgStream.WriteString('NG');
        Db_RisSvr_Irai.func_SaveMsgNG(Db_RisSvr_Irai.wg_Kind, '', w_RecvMsgTime, wg_RecvMsgStream, F_sMsg);
        //3rdログ出力
        proc_LogOut(G_LOG_LINE_HEAD_NG,'',G_LOG_KIND_NG_NUM,F_sMsg);
        //NG電文作成
        Db_RisSvr_Irai.proc_MakeMsg(G_MSG_SYSTEM_A, G_MSGKIND_START,
                                    CST_DENBUNID_NG, ws_Command,
                                    wg_SendMsgStream);
        //NG電文送信
        proc_SendAckStream(Sender,Socket);
        //一単位電文クリア但し現電文は残す 強制読込みのため
        //一単位電文の受信終了を意味する
        ServerRecieveBuf_LenPlan := 0;
        ServerRecieveBuf_Len := 0;
        wg_MaxDataSize := 0;
        //受信領域の初期化
        wg_RecvMsgStream.size := 0;
        //送信領域の初期化
        wg_SendMsgStream.size := 0;
        //受信電文一時退避初期化
        wg_StringStream.size := 0;
        //継続終了
        wg_Continue := False;
        //実電文長（一時退避）初期化
        ServerRecieveBuf_Len_Work := 0;
        //予定電文長（一時退避）初期化
        ServerRecieveBuf_LenPlan_Work := 0;
        //エラーログ文字列初期化
        F_sMsg := '';
        //処理終了
        Exit;
      end
      //すべて正常の場合
      else begin
        //3rdログ出力
        proc_LogOut(G_LOG_LINE_HEAD_OK,'',G_LOG_KIND_SK_SV_NUM,F_sMsg);
        //OK電文作成
        Db_RisSvr_Irai.proc_MakeMsg(G_MSG_SYSTEM_A, G_MSGKIND_START,
                                    CST_DENBUNID_OK, ws_Command,
                                    wg_SendMsgStream);
        //OK電文送信
        proc_SendAckStream(Sender,Socket);

        //2018/08/30 ToHisInfo登録
        Db_RisSvr_Irai.func_InsertToHisInfo();

        //一単位電文クリア但し現電文は残す 強制読込みのため
        //一単位電文の受信終了を意味する
        ServerRecieveBuf_LenPlan := 0;
        ServerRecieveBuf_Len := 0;
        wg_MaxDataSize := 0;
        //受信領域の初期化
        wg_RecvMsgStream.size := 0;
        //送信領域の初期化
        wg_SendMsgStream.size := 0;
        //受信電文一時退避初期化
        wg_StringStream.size := 0;
        //継続終了
        wg_Continue := False;
        //実電文長（一時退避）初期化
        ServerRecieveBuf_Len_Work := 0;
        //予定電文長（一時退避）初期化
        ServerRecieveBuf_LenPlan_Work := 0;
        //エラーログ文字列初期化
        F_sMsg := '';

        //処理終了
        Exit;
      end;
    (*
    end
    else
    begin
      //7.TheraRis_ID特定
      //8.依頼オーダテーブル登録
      //9.電文チェック②

      *** 削除 ***

    *)
    end;
  end
  else
  begin
    //7.患者電文区分特定
    //8.依頼オーダテーブル登録
    //9.電文チェック②
    w_Res := Db_RisSvr_Irai.func_InquireMsgForDB_Patient(G_MSG_SYSTEM_A,
                                                         Db_RisSvr_Irai.wg_Kind,
                                                         wg_RecvMsgStream, w_RecvMsgTime,
                                                         F_sMsg);
    //一単位電文クリア但し現電文は残す 強制読込みのため
    //一単位電文の受信終了を意味する
    ServerRecieveBuf_LenPlan := 0;
    ServerRecieveBuf_Len := 0;
    wg_MaxDataSize := 0;
    //受信領域の初期化
    wg_RecvMsgStream.size := 0;
    //送信領域の初期化
    wg_SendMsgStream.size := 0;
    //受信電文一時退避初期化
    wg_StringStream.size := 0;
    //継続終了
    wg_Continue := False;
    //実電文長（一時退避）初期化
    ServerRecieveBuf_Len_Work := 0;
    //予定電文長（一時退避）初期化
    ServerRecieveBuf_LenPlan_Work := 0;
    //異常終了の場合
    if not w_Res then begin
      //処理異常:FROMHISINFO登録
      wg_RecvMsgStream.Size := 0;
      wg_RecvMsgStream.WriteString('NG');
      Db_RisSvr_Irai.func_SaveMsgNG(Db_RisSvr_Irai.wg_Kind, '', w_RecvMsgTime, wg_RecvMsgStream, F_sMsg);
      //10.2ndログ出力
      proc_LogOut(G_LOG_LINE_HEAD_NG,'',G_LOG_KIND_NG_NUM,F_sMsg);
      //NG電文作成
      Db_RisSvr_Irai.proc_MakeMsg(G_MSG_SYSTEM_A, G_MSGKIND_START,
                                  CST_DENBUNID_NG, ws_Command,
                                  wg_SendMsgStream);
      //11.NG電文送信
      proc_SendAckStream(Sender,Socket);
      //一単位電文クリア但し現電文は残す 強制読込みのため
      //一単位電文の受信終了を意味する
      ServerRecieveBuf_LenPlan := 0;
      ServerRecieveBuf_Len := 0;
      wg_MaxDataSize := 0;
      //受信領域の初期化
      wg_RecvMsgStream.size := 0;
      //送信領域の初期化
      wg_SendMsgStream.size := 0;
      //受信電文一時退避初期化
      wg_StringStream.size := 0;
      //継続終了
      wg_Continue := False;
      //実電文長（一時退避）初期化
      ServerRecieveBuf_Len_Work := 0;
      //予定電文長（一時退避）初期化
      ServerRecieveBuf_LenPlan_Work := 0;
      //エラーログ文字列初期化
      F_sMsg := '';
      //処理終了
      Exit;
    end;
    //10.患者登録
    w_Res := Db_RisSvr_Irai.func_UpdateRisDBPatient(F_sMsg);
    //異常終了の場合
    if not w_Res then begin
      //処理異常:FROMHISINFO登録
      wg_RecvMsgStream.Size := 0;
      wg_RecvMsgStream.WriteString('NG');
      Db_RisSvr_Irai.func_SaveMsgNG(Db_RisSvr_Irai.wg_Kind, '', w_RecvMsgTime, wg_RecvMsgStream, F_sMsg);
      //10.2ndログ出力
      proc_LogOut(G_LOG_LINE_HEAD_NG,'',G_LOG_KIND_NG_NUM,F_sMsg);
      //NG電文作成
      Db_RisSvr_Irai.proc_MakeMsg(G_MSG_SYSTEM_A, G_MSGKIND_START,
                                  CST_DENBUNID_NG, ws_Command,
                                  wg_SendMsgStream);
      //11.NG電文送信
      proc_SendAckStream(Sender,Socket);
      //一単位電文クリア但し現電文は残す 強制読込みのため
      //一単位電文の受信終了を意味する
      ServerRecieveBuf_LenPlan := 0;
      ServerRecieveBuf_Len := 0;
      wg_MaxDataSize := 0;
      //受信領域の初期化
      wg_RecvMsgStream.size := 0;
      //送信領域の初期化
      wg_SendMsgStream.size := 0;
      //受信電文一時退避初期化
      wg_StringStream.size := 0;
      //継続終了
      wg_Continue := False;
      //実電文長（一時退避）初期化
      ServerRecieveBuf_Len_Work := 0;
      //予定電文長（一時退避）初期化
      ServerRecieveBuf_LenPlan_Work := 0;
      //エラーログ文字列初期化
      F_sMsg := '';
      //処理終了
      Exit;
    end
    //すべて正常の場合
    else begin
      //3rdログ出力
      proc_LogOut(G_LOG_LINE_HEAD_OK,'',G_LOG_KIND_SK_SV_NUM,F_sMsg);
      //OK電文作成
      Db_RisSvr_Irai.proc_MakeMsg(G_MSG_SYSTEM_A, G_MSGKIND_START,
                                  CST_DENBUNID_OK, ws_Command,
                                  wg_SendMsgStream);
      //OK電文送信
      proc_SendAckStream(Sender,Socket);

      //2018/08/30 ToHisInfo登録
      Db_RisSvr_Irai.func_InsertToHisInfo();

      //一単位電文クリア但し現電文は残す 強制読込みのため
      //一単位電文の受信終了を意味する
      ServerRecieveBuf_LenPlan := 0;
      ServerRecieveBuf_Len := 0;
      wg_MaxDataSize := 0;
      //受信領域の初期化
      wg_RecvMsgStream.size := 0;
      //送信領域の初期化
      wg_SendMsgStream.size := 0;
      //受信電文一時退避初期化
      wg_StringStream.size := 0;
      //継続終了
      wg_Continue := False;
      //実電文長（一時退避）初期化
      ServerRecieveBuf_Len_Work := 0;
      //予定電文長（一時退避）初期化
      ServerRecieveBuf_LenPlan_Work := 0;
      //エラーログ文字列初期化
      F_sMsg := '';
      //処理終了
      Exit;
    end;
  end;
  //エラーログ文字列初期化
  F_sMsg := '';
end;
//一単位電文の解析と対処
function TRisHisSvrSvc_Irai.func_AnalizeStream(Sender: TObject;
  Socket: TCustomWinSocket):Boolean;
var
  w_Kind:String;
label p_end;

begin
//in:w_RecvArea  ServerRecieveBuf_Len
//out:w_Stream_Ack送信  F_Msg → REdt_RcvCmdData / REdt_RcvAppData
//w_RecvArea現状の受信電文より解析
//受信電文がない時は即時復帰
//サーバ側は受信電文に対する応答は常に固定されている。（ペアーになっている）
  //2.電文チェック
  //初期設定
  Result := True;
  //ステータス表示＆ログ出力
  proc_LogOut(G_LOG_LINE_HEAD_NP, '', G_LOG_KIND_MS_ANLZ_NUM,
              '電文解析開始...');
  //電文読み込みがない場合(電文長-1)
  if ServerRecieveBuf_Len = -1 then
  begin
    //ステータス表示＆ログ出力
    proc_LogOut(G_LOG_LINE_HEAD_NG, '', G_LOG_KIND_NG_NUM,
                '電文解析完了...電文無し-1');
    //異常終了
    Result := False;
    //処理終了
    Exit;
  end;
  //電文読み込みがない場合(電文長0)
  if ServerRecieveBuf_Len = 0 then
  begin
    //ステータス表示＆ログ出力
    proc_LogOut(G_LOG_LINE_HEAD_NG, '', G_LOG_KIND_NG_NUM,
                '電文解析完了...電文無し0');
    //異常終了
    Result := False;
    //処理終了
    Exit;
  end;
  //予定電文長まで読み込めない場合
  if ServerRecieveBuf_Len < ServerRecieveBuf_LenPlan then
  begin
    //ステータス表示＆ログ出力
    proc_LogOut(G_LOG_LINE_HEAD_NG, '', G_LOG_KIND_NG_NUM,
                '電文解析完了...電文不完全');
    //異常終了
    Result := False;
    //処理終了
    Exit;
  end;
  //電文チェック①＋電文分解
  if not Db_RisSvr_Irai.func_InquireMsg(wg_RecvMsgStream, w_Kind, F_sMsg) then
  begin
    //異常終了
    Result := False;
    //処理終了
    Exit;
  end;
  //p_end処理
  goto p_end;
    //処理終了
    Exit;
  p_end:
    //ステータス表示＆ログ出力
    proc_LogOut(G_LOG_LINE_HEAD_OK, '', G_LOG_KIND_MS_ANLZ_NUM,
                '電文解析完了...');
    //処理終了
    Exit;
end;

//一単位電文の解析と対処
procedure TRisHisSvrSvc_Irai.proc_SendAckStream(Sender: TObject;
  Socket: TCustomWinSocket);
var
  w_Buffer : TBuffur;
  res      : Integer;
begin
  //ステータス表示＆ログ出力
  proc_LogOut(G_LOG_LINE_HEAD_NP,'',G_LOG_KIND_SK_SV_NUM,'応答電文送信開始...');
  //送信電文なしの場合
  if wg_SendMsgStream.size = 0 then begin
    //ステータス表示＆ログ出力
    proc_LogOut(G_LOG_LINE_HEAD_NG,'',G_LOG_KIND_NG_NUM,'応答電文送信完了...応答電文なし');
    //処理終了
    Exit;
  end;
  //初期位置に移動
  wg_SendMsgStream.Position := 0;
  //データの読み出し
  wg_SendMsgStream.Read(w_Buffer,wg_SendMsgStream.size);
  //電文書き込み
  res := Socket.SendBuf(w_Buffer,wg_SendMsgStream.size);
  //ステータス表示＆ログ出力
  proc_LogOut(G_LOG_LINE_HEAD_OK,'',G_LOG_KIND_SK_SV_NUM,'応答電文送信...< '+ intToStr(res) +' > ');
end;

//=============================================================================
//↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑
//=============================================================================

//=============================================================================
//初期／終了化処理↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓
//=============================================================================
(**
 -------------------------------------------------------------------
 * @outline  proc_Start
 * @param なし
 * フォーム作成以外のサーバ初期化など通信に必要な初期化処理
 *
 -------------------------------------------------------------------
 *)
procedure TRisHisSvrSvc_Irai.proc_Start;
begin
  //ソケットを閉じる
  ServerSocket1.Close;
  //完全に終了化されるまで待つ
  while ServerSocket1.Active do begin
    //待機時間
    Sleep(1000);
  end;
  //サーバーを開始
  //ポートの設定
  ServerSocket1.Port := StrToInt(Trim(g_S_Socket_Info_01.f_Port));
  //ソケット開始
  ServerSocket1.Open;
  //完全に初期化されるまで待つ
  while not (ServerSocket1.Socket.Connected) do begin
    //待機時間
    Sleep(1000);
  end;
  //ソケットフラグ設定
  wg_SocFlg := True;
end;
(**
 -------------------------------------------------------------------
 * @outline  proc_End
 * @param なし
 * サーバ終了化など通信に必要な終了化処理
 *
 -------------------------------------------------------------------
 *)
procedure TRisHisSvrSvc_Irai.proc_End;
begin
  //ソケットフラグ設定
  wg_SocFlg := False;
  //ソケットを閉じる
  ServerSocket1.Close;
  //完全に終了化されるまで待つ
  while ServerSocket1.Socket.Connected do begin
    //待機時間
    Sleep(1000);
  end;
end;
//=============================================================================
//初期／終了化処理↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑↑
//=============================================================================

initialization
begin
//1)起動PASSを確定
  g_TcpIniPath := ExtractFilePath( ParamStr(0) );
end;

finalization
begin
//
end;

end.
